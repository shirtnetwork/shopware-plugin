{% block sw_cms_element_designer_logo_listing %}
<div class="sw-cms-el-designer-logo-listing">

    <div class="sw-cms-el-designer-logo-listing__content">
        <sw-cms-el-designer-logo
            v-for="index in demoLogoCount"
            :key="index"
            :element="demoLogoElement"
        />
    </div>

    <div class="sw-cms-el-designer-logo-listing__pagination">
        <div class="sw-cms-el-designer-logo-listing__pagination-entry">
            <sw-icon
                name="small-arrow-small-left"
                size="20px"
            />
        </div>

        <div class="sw-cms-el-designer-logo-listing__pagination-entry">
            1
        </div>
        <div class="sw-cms-el-designer-logo-listing__pagination-entry">
            2
        </div>
        <div class="sw-cms-el-designer-logo-listing__pagination-entry">
            3
        </div>

        <div class="sw-cms-el-designer-logo-listing__pagination-entry">
            <sw-icon
                name="small-arrow-small-right"
                size="20px"
            />
        </div>
    </div>
</div>
{% endblock %}
