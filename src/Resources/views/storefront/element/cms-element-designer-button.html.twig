{% block element_designer_button %}
    {% set product = element.data.product %}
    {% if product.customFields.is_shirtnetwork %}
    <div class="cms-element-designer-button">
        {% block element_designer_button_inner %}
            <a href="{{ product.extensions.shirtnetwork.url }}" class="{% if element.config.buttonClass.value %}{{ element.config.buttonClass.value }}{% else %}btn btn-designer btn-primary{% endif %}">
                {{ element.config.label.value }}
            </a>
        {% endblock %}
    </div>
    {% endif %}
{% endblock %}